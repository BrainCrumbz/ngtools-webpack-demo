{
  "name": "webadmin-client",
  "version": "0.1.0",
  "description": "Web client application",
  "private": true,
  "main": "./dist/js/main-bundle.js",
  "scripts": {
    "clean": "rimraf ./dist/*",

    "bundle": "npm run bundle-dev",
      "bundle-dev":  "webpack --config ./webpack.config.dev.js  --display-error-details --display-cached --progress --profile --colors",
      "bundle-watch": "npm run bundle-dev -- --watch",
      "bundle-prod": "webpack --config ./webpack.config.prod.js --display-error-details --display-cached --progress --profile --colors",

    "build": "npm run build-dev",
      "build-dev":  "npm run bundle-dev",
        "prebuild-dev":  "npm run clean",
      "build-watch": "npm run bundle-watch",
        "prebuild-watch-dev": "npm run clean",
      "build-prod": "npm run bundle-prod",
        "prebuild-prod": "npm run clean",

    "start": "npm run serve",

    "serve": "lite-server"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "4.3.4",
    "@angular/compiler": "4.3.4",
    "@angular/core": "4.3.4",
    "@angular/forms": "4.3.4",
    "@angular/http": "4.3.4",
    "@angular/platform-browser": "4.3.4",
    "@angular/platform-browser-dynamic": "4.3.4",
    "@angular/router": "4.3.4",
    "@types/js-base64": "2.1.4",
    "@types/requirejs": "2.1.28",
    "core-js": "~2.4.1",
    "reflect-metadata": "~0.1.10",
    "rxjs": "5.4.3",
    "tslib": "~1.7.1",
    "zone.js": "0.8.16"
  },
  "devDependencies": {
    "@angular/compiler-cli": "4.3.4",
    "@ngtools/webpack": "1.3.3",
    "angular-router-loader": "~0.5.0",
    "angular2-template-loader": "~0.6.2",
    "autoprefixer": "~7.1.1",
    "awesome-typescript-loader": "~3.1.2",
    "copy-webpack-plugin": "~1.1.1",
    "css-loader": "~0.28.2",
    "extract-text-webpack-plugin": "~2.1.0",
    "json-loader": "~0.5.4",
    "lite-server": "2.3.0",
    "node-sass": "~4.5.3",
    "postcss-loader": "~2.0.5",
    "raw-loader": "~0.5.1",
    "rimraf": "~2.5.2",
    "sass-loader": "~6.0.5",
    "source-map-loader": "~0.2.1",
    "style-loader": "~0.18.1",
    "to-string-loader": "~1.1.5",
    "ts-loader": "2.0.3",
    "typescript": "2.4.2",
    "webpack": "2.5.1",
    "webpack-bundle-analyzer": "~2.3.1",
    "webpack-dev-server": "2.4.5"
  }
}
